<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.codepath.gogreen">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ReuseActivity" />
        <activity android:name=".LogReuseActivity"></activity>

        <activity android:name=".RecycleActivity" />
        <activity android:name=".LogRecycleActivity"></activity>
        <activity android:name=".TransitActivity" />
        <activity android:name=".LogTransitActivity" />
        <activity android:name=".WaterActivity" />
        <activity android:name=".LogWaterActivity"></activity>
    </application>

</manifest>